# ignore everything in the "wp-content" directory, except:
# mu-plugins, plugins, and themes directories
*
!themes/
!plugins/

# ignore all mu-plugins, plugins, and themes
# unless explicitly whitelisted at the end of this file
themes/*

# ignore index.php in plugins folder
./plugins/index.php

# ignore all files starting with . or ~
.*
~*

# ignore node dependency directories (used by grunt)
node_modules/

# ignore  composer dependency directories
vendor/

# ignore npm/composer tooling directories
raw
node_modules
nyc_output
coverage
test-treeshake/*-umd.js
amd
cjs
/underscore-node-*-pre*

# ignore OS generated files
ehthumbs.db
Thumbs.db

# ignore Editor files
*.sublime-project
*.sublime-workspace
*.komodoproject
*.codekit3
*.idea
*.swp

# ignore log files and databases
*.log
*.sql
*.sqlite

# ignore compiled files
*.com
*.class
*.dll
*.exe
*.o
*.so

# ignore packaged files
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip

# -------------------------
# BEGIN Whitelisted Files
# -------------------------

# track these files, if they exist
!.gitignore
!.editorconfig
!.scss-lint.yml
!README.md
!CHANGELOG.md
!composer.json

# track these themes
!themes/pasuit/
!themes/pasuit/*

# track these themes
!plugins/akismet/
!plugins/akismet/*

